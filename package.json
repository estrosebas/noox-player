{
  "name": "noox-player",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Noox es un reproductor de música que te permite escuchar música en línea y descargarla.",
  "author": "Sebastian",
  "main": "electron/main.mjs",
  "scripts": {
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:3000 && electron .\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:build": "electron-builder",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "cd src-tauri, npx tauri build"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/cli": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "@capacitor/electron": "^2.5.0",
    "@capacitor/filesystem": "^7.0.0",
    "@capacitor/share": "^7.0.0",
    "@chakra-ui/react": "^3.8.0",
    "@chakra-ui/theme": "^3.4.6",
    "@chakra-ui/theme-tools": "^2.2.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@react-oauth/google": "^0.12.1",
    "@tanstack/react-query": "^5.66.0",
    "@tauri-apps/api": "^2.2.0",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "capacitor-music-controls-plugin": "^6.1.0",
    "cors": "^2.8.5",
    "downshift": "^9.0.8",
    "express": "^4.21.2",
    "framer-motion": "^12.4.3",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-modal": "^3.16.3",
    "react-toastify": "^11.0.3",
    "sweetalert2": "^11.17.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tauri-apps/cli": "^2.2.7",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-modal": "^3.16.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "icon-gen": "^5.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.tuempresa.nooxmusic",
    "productName": "Noox Music",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "icon": "build/noox.ico"
    }
  }
}
